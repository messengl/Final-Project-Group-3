<table>
	<tr id= "category">
	
	{{!-- CATEGORY HEADERS HERE --}}
	{{#each categories}}
		<th>
			{{this}}
		</th>
	{{/each}}
	
	</tr>
	
	{{!--REST OF DATA HERE--}}
	{{#each data}}
		<tr class = "data-row">
		
		{{#each this }}
			<td class = data> {{ this }} </td>
		{{/each}}
		
		</tr>
	{{/each}}

</table>